<Window x:Class="WpfApp1.form.QueryResultForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.form"
        mc:Ignorable="d"
        Title="QueryResultForm" Height="500" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="43*"/>
        </Grid.RowDefinitions>
        <ComboBox Name="cb_result" Grid.Row="0" Margin="5"/>
        <DataGrid Name="grid_result" Grid.Row="1" Margin="5"/>
    </Grid>
</Window>
